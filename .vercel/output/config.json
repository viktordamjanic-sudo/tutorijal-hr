{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/favorite/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/generate/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/progress/?$",
			"dest": "_render"
		},
		{
			"src": "^/lekcija/1/?$",
			"dest": "_render"
		},
		{
			"src": "^/lekcija/2/?$",
			"dest": "_render"
		},
		{
			"src": "^/lekcija/3/?$",
			"dest": "_render"
		},
		{
			"src": "^/lekcija/4/?$",
			"dest": "_render"
		},
		{
			"src": "^/lekcija/5/?$",
			"dest": "_render"
		},
		{
			"src": "^/modul/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/profil/?$",
			"dest": "_render"
		},
		{
			"src": "^/sign-in/?$",
			"dest": "_render"
		},
		{
			"src": "^/sign-up/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}